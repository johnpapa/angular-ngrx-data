{
  "name": "angular-ngrx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev-proxy": "concurrently \"NODE_ENV=development npm run debug\" \"ng serve --proxy-config proxy.conf.json --open\"",
    "ng": "ng",
    "start-ng": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --build-optimizer",
    "sme": "./node_modules/.bin/source-map-explorer",
    "gen-stats": "ng build --prod --stats-json",
    "gen-report": "webpack-bundle-analyzer dist/publicweb/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker-up": "docker-compose up -d --build",
    "docker-up-debug": "docker-compose -f docker-compose.debug.yml up -d --build",
    "docker-down": "docker-compose down",
    "start": "NODE_ENV=development PUBLICWEB=./dist/publicweb node src/server/index.js",
    "debug": "NODE_ENV=development PUBLICWEB=./dist/publicweb node --inspect src/server/index.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "body-parser": "~1.17.2",
    "core-js": "^2.4.1",
    "express": "~4.15.3",
    "mongoose": "~4.11.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.3",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@ngrx/store-devtools": "^4.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "source-map-explorer": "^1.5.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}

{
  "name": "angular-ngrx-data",
  "version": "0.2.12",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build-prod": "ng build --prod --build-optimizer",
    "build-lib": "ng-packagr -p lib",
    "build-all": "rm -rf dist && npm run build-setup && npm run build-prod",
    "build-setup":
      "npm run build-lib && npm install dist/ngrx-data --no-save --no-package-lock",
    "build-publish": "npm run build-lib && npm publish dist/ngrx-data",
    "sme": "./node_modules/.bin/source-map-explorer",
    "gen-stats": "ng build --prod --stats-json",
    "gen-report": "webpack-bundle-analyzer dist/publicweb/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker-up": "docker-compose up -d --build",
    "docker-up-debug":
      "docker-compose -f docker-compose.debug.yml up -d --build",
    "docker-down": "docker-compose down",
    "dev":
      "concurrently \"npm run debug\" \"ng serve --proxy-config proxy.conf.json --open\"",
    "start-server": "node ./src/server/index.js",
    "debug":
      "PUBLICWEB=./dist/publicweb node -r dotenv/config --inspect src/server/index.js",
    "serve-dist":
      "PUBLICWEB=./dist/publicweb node -r dotenv/config --inspect dist/index.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "precommit": "pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/entity": "^5.1.0",
    "@ngrx/store": "^5.1.0",
    "angular-in-memory-web-api": "^0.5.4",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.0",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.0.11",
    "mongoose": "~4.11.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@ngrx/store-devtools": "^5.1.0",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.1.0",
    "concurrently": "^3.5.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-marbles": "^0.2.0",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.2.0",
    "ngrx-store-freeze": "^0.2.0",
    "prettier": "1.11.1",
    "pretty-quick": "^1.4.1",
    "protractor": "~5.1.2",
    "snyk": "^1.52.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.8.0",
    "typescript": "~2.4.2"
  },
  "snyk": true
}
